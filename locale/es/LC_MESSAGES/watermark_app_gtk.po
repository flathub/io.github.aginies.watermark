msgstr "Opciones Expertas"

#: watermark_app_gtk.py:391
msgid "Angle (degrees)"
msgstr "Ángulo (grados)"

#: watermark_app_gtk.py:397
msgid "Watermark Angle in the image (positive and negative)"
msgstr "Ángulo de la marca de agua en la imagen (positivo y negativo)"

#: watermark_app_gtk.py:406
msgid "Transparency"
msgstr "Transparencia"

#: watermark_app_gtk.py:413
msgid "<b>* 0:</b> No transparency\n<b>* 100:</b> Invisible"
msgstr "<b>* 0:</b> Sin transparencia\n<b>* 100:</b> Invisible"

#: watermark_app_gtk.py:422
msgid "Density"
msgstr "Densidad"

#: watermark_app_gtk.py:428
msgid "<b>* 0:</b> None\n<b>* 200:</b> Fill the image"
msgstr "<b>* 0:</b> Ninguna\n<b>* 200:</b> Llenar la imagen"

#: watermark_app_gtk.py:441
msgid "Random Colors"
msgstr "Colores Aleatorios"

#: watermark_app_gtk.py:451
msgid "Filename Prefix"
msgstr "Prefijo del Nombre de Archivo"

#: watermark_app_gtk.py:454
msgid "You can add an <b>Extra Prefix</b> to the final filename"
msgstr "Puedes agregar un <b>prefijo adicional</b> al nombre de archivo final"

#: watermark_app_gtk.py:463
msgid "Include Date + Hour"
msgstr "Incluir Fecha y Hora"

#: watermark_app_gtk.py:466
msgid "Add the Date and the Hour in the watermark, include that into the filename"
msgstr "Agregar la fecha y la hora en la marca de agua, incluir eso en el nombre de archivo"

#: watermark_app_gtk.py:474
msgid "Resize to"
msgstr "Redimensionar a"

#: watermark_app_gtk.py:476
msgid "You can <b>resize</b> the original image to another size.\nThis option is not available if you choose PDF output format."
msgstr "Puedes <b>redimensionar</b> la imagen original a otro tamaño.\nEsta opción no está disponible si eliges el formato de salida PDF."

#: watermark_app_gtk.py:489
msgid "JPEG (%)"
msgstr "JPEG (%)"

#: watermark_app_gtk.py:496
msgid "<b>* 0:</b> None\n<b>* 100:</b> Full compression"
msgstr "<b>* 0:</b> Ninguna\n<b>* 100:</b> Compresión completa"

#: watermark_app_gtk.py:499
msgid "PDF"
msgstr "PDF"

#: watermark_app_gtk.py:500
msgid "Save the image into a <b>PDF</b> format instead of JPEG.\nThere is no view of the watermarked file in PDF format."
msgstr "Guardar la imagen en formato <b>PDF</b> en lugar de JPEG.\nNo hay vista previa del archivo con marca de agua en formato PDF."

#: watermark_app_gtk.py:515
msgid "Add Watermark"
msgstr "Agregar Marca de Agua"

#: watermark_app_gtk.py:592
msgid "Could not find the default font file, choose another one."
msgstr "No se pudo encontrar el archivo de fuente predeterminado, elige otro."

#: watermark_app_gtk.py:654
msgid "Choose a TTF Font"
msgstr "Elegir una Fuente TTF"

#: watermark_app_gtk.py:694
msgid "An error occurred, can't find the font path, please choose another one"
msgstr "Ocurrió un error, no se puede encontrar la ruta de la fuente, por favor elige otra"

#: watermark_app_gtk.py:832
msgid "Watermark App Version 4.1"
msgstr "Aplicación de Marca de Agua Versión 4.1"

#: watermark_app_gtk.py:840
msgid "This app add a Watermark to images\nOpen Source Project\nLicence GPL2"
msgstr "Esta aplicación agrega una marca de agua a las imágenes\nProyecto de Código Abierto\nLicencia GPL2"

